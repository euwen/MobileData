            </TableRow>
        </TableLayout>

        <LinearLayout
            android:id="@+id/mag_curve"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"></LinearLayout>
    </LinearLayout>


</LinearLayout>